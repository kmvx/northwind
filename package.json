{
  "name": "northwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm:*:watch\"",
    "dev": "vite",
    "dev:watch": "vite --clearScreen=false",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:check": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier:check": "prettier --check .",
    "prettier": "prettier --write .",
    "prettier:watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "format:check": "npm run lint:check && npm run prettier:check",
    "format": "npm run lint && npm run prettier",
    "vitest": "vitest",
    "vitest:run": "vitest --run",
    "vitest:update": "vitest --update --run",
    "vitest:typecheck": "vitest --typecheck",
    "vitest:typecheck:run": "vitest --typecheck --run",
    "test": "npm run tsc && npm run format && npm run vitest:typecheck:run && npm run vitest:run && echo OK",
    "pw": "playwright test --forbid-only",
    "pw:rep": "playwright show-report",
    "pw:chrome": "playwright test --project=chromium",
    "pw:ui": "playwright test --ui --headed",
    "pw:debug": "playwright test --project=chromium --debug",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-query-devtools": "^5.72.2",
    "axios": "^1.8.4",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.8",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "react-paginate": "^8.3.0",
    "react-router-dom": "^7.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.35.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "onchange": "^7.1.0",
    "prettier": "^3.5.3",
    "sass": "=1.70.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "vitest": "^3.1.1"
  }
}
